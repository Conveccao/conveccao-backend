          }
        }
      }
    },
    "/sensors": {
      "post": {
        "tags": [
          "Sensores"
        ],
        "summary": "Cadastra um sensor no banco de dados",
        "description": "envia os dados de um sensor para serem registrados em um banco de dados.",
        "operationId": "RegisterSensor",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "required": true,
            "schema": {
              "type": "array",
