    "/sensors/:id": {
      "get": {
        "tags": [
          "Sensores"
        ],
        "summary": "Busca um determinado um sensor especifico registrado",
        "description": "Ultiliza a requisição Get para retornar um sensor por ID.",
        "operationId": "getSensoresiD",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "302": {
            "description": "retorna as sensores registradas",
            "schema": {
              "$ref": "#/definitions/Sensor"
            }
          }
        }
      }
    },
    "/sensors/:id (delete)": {
      "delete": {
        "tags": [
          "Sensores"
        ],
        "summary": "Deleta um sensor",
        "description": "através da id, remove um sensor cadastrado no banco de dados da aplicação",
