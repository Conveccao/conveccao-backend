        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "dados que serão atualizados",
            "required": true,
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SensorID"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "retorna o Sensor com os dados alterados"
          }
        }
      }
    }
