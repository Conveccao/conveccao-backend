        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "ID do Sensor",
            "in": "body",
            "description": "Id do Sensor que será deleta",
            "required": true,
            "schema": {
              "items": {
                "$ref": "#/definitions/SensorID"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Ok"
          },
          "400": {
            "description": "ID de Sensor inválido"
          },
          "404": {
            "description": "Chamado not found"
          }
        }
